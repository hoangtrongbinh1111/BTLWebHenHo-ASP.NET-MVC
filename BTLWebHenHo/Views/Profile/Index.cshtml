
@{
     ViewBag.Tittle = "My Profile";
     Layout = null;
     var info = ViewBag.info;
     var birthday = "";
     var age = 0;
     if (info.birthday != null)
     {
          string[] spl = info.birthday.Split('-');
          if (spl.Count() > 1)
          {
               birthday = spl[0];
               age = DateTime.Now.Year - Convert.ToInt32(birthday);
               birthday = spl[2] + "/" + spl[1] + "/" + spl[0];
          }
          else
          {
               string[] spl1 = info.birthday.Split('/');
               birthday = spl1[2];
               age = DateTime.Now.Year - Convert.ToInt32(birthday);
               birthday = spl1[0] + "/" + spl1[1] + "/" + spl1[2];

          }

     }
     var list_user_chat = ViewBag.list_user_chat;
     //cal percent profile done
     var cal_percent = ViewBag.cal_percent;
}

